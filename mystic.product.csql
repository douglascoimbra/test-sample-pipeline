SELECT
  to_hex(
    md5(
      concat(
        ifnull(stg.id, ''), 
        ifnull(stg.productname, ''), 
        ifnull(stg.category, ''), 
        ifnull(cast(stg.productprice AS string), '')
      )
    )
  ) AS __mdmid, 
  contains_substr(stg.productname, 'banan') AS mdmdeleted
  --metadataNoId--
FROM 
  stg_nlp_product AS stg 
WHERE 
  1 = 1
  --timestamp-- and stg.mdmCounterForEntity > {{start_from}}
  --tenantIds-- and mdmTenantId IN {{tenantIds}}
